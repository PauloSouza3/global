Table CadastroCliente {
  cpf int [pk]
  nome varchar
  email varchar
  data_nascimento date
 
}


Table LoginCliente {
  email varchar [pk]
  nome_usuario varchar 
  senha varchar
  
}


Table CadastroPsicologo {
  cpf_psi int [pk]
  nome varchar
  email varchar
  data_nascimento date
  
}


Table LoginPsicologos {

   nome_psicologo varchar [pk]
  email varchar
  senha varchar
  }






